{
  "name": "reqly",
  "version": "0.2.0",
  "description": "Express helper to quickly run a server with Babelify, LESS and SASS",
  "main": "app/index.js",
  "scripts": {
    "test": "eslint *.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bmson/reqly.git"
  },
  "keywords": [
    "server",
    "express",
    "websocket"
  ],
  "author": "Baldvin Smarason <bmson@bmson.com>",
  "licenses": "MIT",
  "bugs": {
    "url": "https://github.com/bmson/reqly/issues"
  },
  "dependencies": {
    "express": "4.13.x",
    "ws": "1.0.x",
    "express-less": "0.0.x",
    "node-sass-middleware": "0.9.x",
    "browserify-middleware": "7.0.x",
    "babel-preset-es2015": "6.x.x",
    "babelify": "7.2.x"
  },
  "devDependencies": {
    "eslint": "x.x.x"
  },
  "eslintConfig": {
    "rules": {
        "indent": [2, 2],
        "quotes": [2, "single"],
        "linebreak-style": [2, "unix"],
        "semi": [2, "always"]
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "extends": "eslint:recommended"
  }
}
